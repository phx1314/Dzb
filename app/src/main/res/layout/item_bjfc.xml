<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="center"
    android:orientation="horizontal">

    <LinearLayout
        android:id="@+id/mLinearLayout_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/j10dp"
        android:layout_weight="1"
        android:background="@drawable/bg_banjizhaopian"
        android:orientation="vertical"
        android:paddingLeft="@dimen/j3dp"
        android:paddingRight="@dimen/j5dp">

        <com.mdx.framework.widget.MImageView
            android:id="@+id/mImageView1"
            android:layout_width="match_parent"
            android:layout_height="@dimen/j60dp"
            android:scaleType="fitXY"
            android:src="@drawable/default_avatar" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/j20dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/mTextView_name_1"
                style="@style/text_style_13_black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textStyle="bold" />

            <TextView
                android:id="@+id/mTextView_num_1"
                style="@style/text_style_13_black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/j5dp"
                android:text=""
                android:textColor="#6b7b92" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/mLinearLayout_2"
        android:layout_width="0dp"
